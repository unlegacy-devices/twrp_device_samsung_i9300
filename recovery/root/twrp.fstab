/cache          ext4    /dev/block/platform/dw_mmc/by-name/CACHE       flags=display="Cache";backup=1
/data           ext4    /dev/block/platform/dw_mmc/by-name/USERDATA    flags=display="Data";encryptable=footer;length=-16384;backup=1
/system         ext4    /dev/block/platform/dw_mmc/by-name/SYSTEM      flags=display="System";backup=1
/system_image   emmc    /dev/block/platform/dw_mmc/by-name/SYSTEM      flags=display="System Image";backup=1;flashimg=1
/boot           emmc    /dev/block/platform/dw_mmc/by-name/BOOT        flags=display="Boot";backup=1
/recovery       emmc    /dev/block/platform/dw_mmc/by-name/RECOVERY    flags=display="Recovery";backup=1
/firmware       vfat    /dev/block/platform/dw_mmc/by-name/RADIO     flags=display="Firmware";backup=1
/modem			vfat    /dev/block/platform/dw_mmc/by-name/RADIO         flags=display="Modem";backup=1
/efs            ext4    /dev/block/platform/dw_mmc/by-name/EFS         flags=display="EFS";backup=1
/misc           emmc    /dev/block/platform/dw_mmc/by-name/OTA        flags=display="Misc"

/external_sd    auto    /dev/block/mmcblk1p1    /dev/block/mmcblk1         flags=display="MicroSD";storage;wipeingui;removable
/usb-otg        auto    /dev/block/sda1         /dev/block/sda             flags=display="USB OTG";storage;wipeingui;removable
